{
  "name": "electron-typescript-react-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate project for electron using typescript, sass, react, jest, spectron, web pack and redux",
  "main": "build/main.js",
  "dependencies": {
    "electron-window-state": "^5.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^23.3.14",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^6.0.14",
    "@types/webdriverio": "^4.13.3",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.1",
    "electron": "^4.2.8",
    "electron-packager": "^13.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "html-webpack-plugin": "^3.2.0",
    "import-glob-loader": "^1.1.0",
    "jest": "^23.6.0",
    "node-sass": "^4.12.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.4.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "scripts": {
    "build": "npm run test && webpack --config webpack.config.prod.js",
    "build:watch": "npm run build -- --watch",
    "start": "electron .",
    "restart": "npm run build && npm start",
    "dev": "webpack-dev-server --open --config webpack.config.dev.js",
    "dev:hot": "npm run dev -- --hot",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "pack": "node packageApp.js",
    "distribute": "npm install && npm run build && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kelvinoo7/electron-typescript-react-boilerplate.git"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "sass",
    "create electron app",
    "jest",
    "spectron",
    "startup"
  ],
  "author": "Kelvin Kamani <kamani_kelvin@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kelvinoo7/electron-typescript-react-boilerplate/issues"
  },
  "homepage": "https://github.com/Kelvinoo7/electron-typescript-react-boilerplate#readme"
}
