{
  "name": "electron-typescript-react-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate project for electron using typescript, sass, react  jest, web pack and redux",
  "main": "dist/main.js",
  "dependencies": {
    "electron-window-state": "^5.0.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.2",
    "electron": "^4.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "html-webpack-plugin": "^3.2.0",
    "import-glob-loader": "^1.1.0",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.13",
    "webpack-merge": "^4.1.5"
  },
  "scripts": {
    "build": "npm run test && webpack --config webpack.config.prod.js",
    "build:watch": "npm run build -- --watch",
    "start": "electron .",
    "restart": "npm run build && npm start",
    "dev": "webpack-dev-server --open --config webpack.config.dev.js",
    "dev:hot": "npm run dev -- --hot",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kelvinoo7/electron-typescript-react-boilerplate.git"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "sass",
    "create electron app",
    "startup"
  ],
  "author": "Kelvin Kamani <kamani_kelvin@yahoo.com>",
